# Django Project .gitignore

# Environment variables (contains sensitive data like SECRET_KEY, database passwords)
.env
.env.local
.env.production

# Database files
*.sqlite3
*.db
db.sqlite3

# Media files (user uploaded content)
media/
# If you want to keep media folder structure but ignore contents:
# media/*
# !media/.gitkeep

# Python compiled files
__pycache__/
*.py[cod]
*$py.class
*.so

# Django migrations (keep initial migrations, ignore auto-generated ones)
*/migrations/0*.py
!*/migrations/__init__.py
!*/migrations/0001_initial.py

# Static files collected for production
staticfiles/
static/

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Virtual environment
venv/
env/
ENV/
.venv/

# Logs
*.log
logs/

# Coverage reports
htmlcov/
.coverage
.coverage.*
coverage.xml

# pytest
.pytest_cache/

# Jupyter Notebook
.ipynb_checkpoints

# pyenv
.python-version

# Temporary files
*.tmp
*.temp